{
    "name": "warehouse",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon -r esm src/warehouse.js",
        "clean": "rm -r dist",
        "build": "babel src -d dist && cp -r src/views dist && cp -r public dist",
        "deploy": "npm run clean && npm run build && . ~/cli/remote-redeploy-warehouse.sh"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-parser": "~1.4.4",
        "core-js": "^3.2.1",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "express": "~4.16.1",
        "express-session": "^1.16.2",
        "http-errors": "~1.6.3",
        "i18n": "^0.13.2",
        "moment": "^2.24.0",
        "mongoose": "^5.7.1",
        "morgan": "~1.9.1",
        "multer": "^1.4.2",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^5.0.1",
        "pug": "^2.0.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.2",
        "@babel/core": "^7.6.2",
        "@babel/node": "^7.6.2",
        "@babel/preset-env": "^7.6.2",
        "nodemon": "^1.19.2"
    }
}
